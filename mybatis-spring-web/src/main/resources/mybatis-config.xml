<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="model.Comment" alias="Comment" />
		<typeAlias type="model.Product" alias="Product" />
		<typeAlias type="model.Member" alias="Member" />
		<typeAlias type="model.CustomService" alias="CustomService" />
		<typeAlias type="model.BuyList" alias="BuyList" />
		<typeAlias type="model.BuyProduct" alias="BuyProduct" />
		<typeAlias type="model.Linkdata" alias="Linkdata" />
		<typeAlias type="model.Payment" alias="Payment" />
		<typeAlias type="model.Bucketlist" alias="Bucketlist" />
		<typeAlias type="model.Reservation" alias="Reservation" />
		<typeAlias type="model.Reservation_Bucketlist" alias="Reservation_Bucketlist" />
		<typeAlias type="model.Bucket_Review" alias="Bucket_Review" />
		<typeAlias type="model.JoinBuyListABuyProduct" alias="JoinBuyListABuyProduct" />
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="oracle.jdbc.driver.OracleDriver" />
				<property name="url" value="jdbc:oracle:thin:@localhost:1521:orcl" />
				<property name="username" value="GrabTest" />
				<property name="password" value="1234" />
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<mapper resource="repository/mapper/CommentMapper.xml" />
		<mapper resource="repository/mapper/ProductMapper.xml" />
		<mapper resource="repository/mapper/PwmailMapper.xml" />
		<mapper resource="repository/mapper/PaymentMapper.xml" />
		<mapper resource="repository/mapper/CsMapper.xml" />
		<mapper resource="repository/mapper/LoginMapper.xml" />
		<mapper resource="repository/mapper/GraphMapper.xml" />
		<mapper resource="repository/mapper/BuyProductMapper.xml" />
		<mapper resource="repository/mapper/BuyListMapper.xml" />
		<mapper resource="repository/mapper/BucketMapper.xml" />
		<mapper resource="repository/mapper/JoinBuyListABuyProductMapper.xml" />
	</mappers>
</configuration>